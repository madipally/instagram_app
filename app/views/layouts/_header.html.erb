<header class="navbar navbar-fixed-top navbar-inverse">
    <div class="container">
        <%= link_to "Instagram", '#', id: "logo" %>
        <nav>
            <ul class="nav navbar-nav navbar-right">
                <% if user_signed_in? %>
                    <li><%=link_to "Logout", destroy_user_session_path, method: :delete %></li>
                <% else %>
                    <li><%= link_to "Sign in with Instagram", user_instagram_omniauth_authorize_path %></li> 
                <% end %>
            </ul>
        </nav>
    </div>
</header>
    